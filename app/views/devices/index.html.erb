<% provide(:title, 'All devices') %>
<%= link_to "导出装置数据" , for_device_path %>
<% provide(:button_text, '导入文件') %>
<div class="btn-data" style="width:150px;height:10px;border:none;outline:none;">
<%= form_for(current_user) do |f| %>
    <p><%= f.label "请输入导入的文件" %></p>
    <%= f.file_field :device_adrr ,style: "width:147px;height:10px;border:none;outline:none;"%> 
    <%= f.submit yield(:button_text), class: "btn btn-primary" ,style: "width:147px" %>
<% end %>
</div>
<h1>所有装置 (<%= @devices.count %>)</h1>
<%= will_paginate %>

<ul class="devices">
    <%= render @devices %>
</ul> 

<%= will_paginate %>

